<p id="notice"><%= notice %></p>

<h1><%= t :Municipalities %></h1>

<ul>
  <% @municipalities.each do |municipality| %>
    <li><%= link_to municipality.name, municipality %></li>
  <% end %>
</ul><br>

<% if can? :create, Municipality %>
    <%= link_to (t :'New_Municipality'), new_municipality_path, class:'btn btn-default' %>
<% end %>